{
  "extends": ["eslint:recommended", "airbnb", "airbnb/hooks"],
  "env": {
    "es6": true,
    "browser": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 9,
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {"import/extensions": ["error", "always", {"ignorePackages": true}]}
}